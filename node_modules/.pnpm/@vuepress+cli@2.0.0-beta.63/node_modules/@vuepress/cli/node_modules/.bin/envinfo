#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/envinfo@7.8.1/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/envinfo@7.8.1/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../envinfo@7.8.1/node_modules/envinfo/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../envinfo@7.8.1/node_modules/envinfo/dist/cli.js" "$@"
fi
