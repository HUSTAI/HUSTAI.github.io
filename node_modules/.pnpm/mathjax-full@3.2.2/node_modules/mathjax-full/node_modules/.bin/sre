#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/speech-rule-engine@4.0.7/node_modules/speech-rule-engine/bin/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/speech-rule-engine@4.0.7/node_modules/speech-rule-engine/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/speech-rule-engine@4.0.7/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/speech-rule-engine@4.0.7/node_modules/speech-rule-engine/bin/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/speech-rule-engine@4.0.7/node_modules/speech-rule-engine/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/speech-rule-engine@4.0.7/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../speech-rule-engine@4.0.7/node_modules/speech-rule-engine/bin/sre" "$@"
else
  exec node  "$basedir/../../../../../speech-rule-engine@4.0.7/node_modules/speech-rule-engine/bin/sre" "$@"
fi
