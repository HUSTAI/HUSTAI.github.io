else
  export NODE_PATH="/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/vuepress-vite@2.0.0-beta.63_@vuepress+client@2.0.0-beta.63_vue@3.3.4/node_modules/vuepress-vite/bin/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/vuepress-vite@2.0.0-beta.63_@vuepress+client@2.0.0-beta.63_vue@3.3.4/node_modules/vuepress-vite/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/vuepress-vite@2.0.0-beta.63_@vuepress+client@2.0.0-beta.63_vue@3.3.4/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vuepress-vite/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../vuepress-vite/bin/vuepress.js" "$@"
