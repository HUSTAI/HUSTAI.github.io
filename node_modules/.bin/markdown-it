#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/markdown-it/bin/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/markdown-it/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules:/home/root1/下载/chrome_download/June/node_modules:/home/root1/下载/chrome_download/node_modules:/home/root1/下载/node_modules:/home/root1/node_modules:/home/node_modules:/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/markdown-it/bin/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/markdown-it/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules:/home/root1/下载/chrome_download/June/node_modules:/home/root1/下载/chrome_download/node_modules:/home/root1/下载/node_modules:/home/root1/node_modules:/home/node_modules:/node_modules:/home/root1/下载/chrome_download/June/hust-404.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../markdown-it/bin/markdown-it.js" "$@"
else
  exec node  "$basedir/../markdown-it/bin/markdown-it.js" "$@"
fi
